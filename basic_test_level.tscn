[sub_resource type="GDScript" id="GDScript_rtkpm"]
script/source = "extends Area2D

@export var effects_array : Array[EffectedProperties]
var enemies_in_area : Array = []

func _process(delta):
	$Label.text = str(enemies_in_area)

func create_effect() -> Effect:
	var slow_effect = Effect.new()
	for property in effects_array:
		var slow_effect_res = EffectedProperties.new()
		slow_effect_res.property_name = property.property_name
		slow_effect_res.modificator_mode = property.modificator_mode
		slow_effect_res.property_modificator = property.property_modificator
		slow_effect.properties_array.append(slow_effect_res)
	return slow_effect

func _on_area_entered(enemy_area):
	var array_of_effects : Array = []
	var enemy = enemy_area.get_parent()
	
	for nested_array in enemies_in_area:
		if enemy in nested_array:
			assert(enemy == null) # Fallback if the enemy already is inside this area (Probabbly a bug, so assert)
			return
	
	var created_effect = create_effect()
	array_of_effects.append(created_effect)
	enemy.add_child(created_effect)
	
	var enemy_array : Array = [enemy, array_of_effects]
	enemies_in_area.append(enemy_array)


func _on_area_exited(enemy_area):
	var enemy = enemy_area.get_parent()
	
	for nested_array in enemies_in_area:
		if nested_array[0] == enemy:
			for effect in nested_array[1]:
				if effect != null:
					effect.queue_free()
			enemies_in_area.erase(nested_array)
"

